    "@angular/compiler": "^2.0.2",
    "@angular/core": "^2.0.2",
    "@angular/forms": "^2.0.2",
    "@angular/http": "^2.0.2",
    "@angular/material": "^2.0.0-alpha.10",
    "@angular/platform-browser": "^2.0.2",
    "@angular/platform-browser-dynamic": "^2.0.2",
    "@angular/router": "^3.0.2",
    "@angular/upgrade": "^2.0.2",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.14",
    "angular-in-memory-web-api": "^0.1.16",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "canonical-path": "0.0.2",
    "commitizen": "^2.8.6",
    "concurrently": "^3.1.0",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "cz-conventional-changelog": "^1.2.0",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.1",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "http-server": "^0.9.0",
    "istanbul": "^0.4.5",
