{
  "name": "cleoharmony",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" \"nodemon app.js\"",
    "backend-clean": "rimraf coverage dist",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "commit": "git-cz",
    "frontend-unit": "karma start karma.conf.js",
    "frontend-coverage": "http-server -c-1 -o -p 9875 ./FE_unitTest_Coverage/coverage",
    "backend-unit": "node_modules/.bin/mocha -R spec spec.js",
    "backend-coverage": "npm run backend-clean && node_modules/.bin/istanbul cover node_modules/.bin/_mocha spec.js"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/abhinav-cleo/harmonyangular2project"
  },
  "devDependencies": {
